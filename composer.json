{
    "name": "kolayik/kolay-php-client",
    "description": "Kolay API client for PHP",
    "keywords": ["kolayik", "kolayik.com", "kolay.io", "kolay"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kolay Dev Team",
            "homepage": "https://kolay.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kolay\\" : "src"
        }
    },
    "require": {
        "php": ">= 7.1",
        "ext-json": "*",
        "php-http/httplug": "^1.1.0",
        "php-http/client-implementation": "*",
        "php-http/discovery": "^1.4",
        "php-http/message": "^1.7",
        "psr/http-message": "^1.0",
        "php-http/client-common": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.1",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0"
    }
}
